{
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules"
    ],
    "compilerOptions": {
        "noImplicitAny": true,
        "target": "es5",
        "lib":["es5"],
        "moduleResolution": "Node",
        "typeRoots": [
            "./node_modules/@types"
        ],
        "types": [
            "node"
          ]
    },
    "allowSyntheticDefaultImports": true
}